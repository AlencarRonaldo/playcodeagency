{
  "name": "playcode-agency",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "dev:clean": "npm run cache:clear && next dev",
    "build": "next build",
    "build:vercel": "bash build-vercel.sh",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "cache:clear": "node scripts/cache-monitor.js clear",
    "cache:reset": "npm run cache:clear && npm install",
    "cache:check": "node scripts/cache-monitor.js check",
    "cache:monitor": "node scripts/cache-monitor.js monitor",
    "cache:size": "node scripts/cache-monitor.js size",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "deploy:test": "bash scripts/deploy.sh test",
    "deploy:vercel": "bash scripts/deploy.sh vercel",
    "deploy:production": "bash scripts/deploy.sh production",
    "production:setup": "cp .env.production.example .env.production && echo 'Configure .env.production antes do deploy!'",
    "production:keys": "echo 'TOKEN_SECRET_KEY:' && openssl rand -hex 32 && echo 'ADMIN_TOKEN:' && openssl rand -hex 16",
    "health:check": "curl -f http://localhost:3000/api/health || echo 'Server not running'",
    "build:hostinger": "copy next.config.ts next.config.backup.ts && copy next.config.hostinger.ts next.config.ts && next build && copy next.config.backup.ts next.config.ts && del next.config.backup.ts",
    "hostinger:build": "npm run build:hostinger",
    "hostinger:deploy": "scripts/deploy-hostinger.bat",
    "hostinger:clean": "rm -rf .next out && npm run build:hostinger",
    "test:purchase": "playwright test purchase-flow purchase-scenarios",
    "test:examples": "playwright test examples/",
    "test:report": "playwright show-report",
    "test:install": "playwright install",
    "test:ci": "playwright test --reporter=junit",
    "test:coverage": "playwright test --reporter=html"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@react-three/drei": "^10.5.1",
    "@react-three/fiber": "^9.2.0",
    "@types/nodemailer": "^6.4.17",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.6",
    "html2canvas": "^1.4.1",
    "isomorphic-dompurify": "^2.26.0",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.525.0",
    "next": "15.4.2",
    "nodemailer": "^7.0.5",
    "openai": "^5.10.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1",
    "three": "^0.178.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.48.2",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.4.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
